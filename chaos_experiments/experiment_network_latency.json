{
    "title": "Uji Ketahanan Terhadap Latensi Jaringan di AWS",
    "description": "Menambahkan delay 300ms pada trafik jaringan VM AWS menggunakan Ansible.",
    "method": [
        {
            "type": "action",
            "name": "add-network-latency-via-ansible",
            "provider": {
                "type": "process",
                "path": "ansible-playbook",
                "args": "-i inventory/ playbooks/chaos/manage_latency.yml -e \"state=present\""
            }
        }
    ],
    "rollbacks": [
        {
            "type": "action",
            "name": "remove-network-latency-via-ansible",
            "provider": {
                "type": "process",
                "path": "ansible-playbook",
                "args": "-i inventory/ playbooks/chaos/manage_latency.yml -e \"state=absent\""
            }
        }
    ]
}